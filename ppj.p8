-- enemy

function spawn_enemy(level_tbl)
	start_y=128*4-60
	for i=1,level_tbl.num_enemy do
		add(enemies, {
			sp=32,
			x=level*128+rnd(104)+16,
			y=start_y-i*48,
			dy=0,
			w=8,
			h=8,
			flipped=false,
			anim=0
		})
	end
end

function draw_enemy(enemies)
	for k,v in pairs(enemies) do
		spr(v.sp,v.x,v.y,1,1,v.flipped)
	end
end

function enemy_atk(enemies)
	for k,v in pairs(enemies) do
		if v.flipped then
			add(enemy_attacks, {
				sp=48,
				x=v.x-8,
				y=v.y,
				dx=0,
				w=8,
				h=8,
				anim=0,
				active=true,
				positive=v.flipped
			})
		else
			add(enemy_attacks, {
				sp=48,
				x=v.x+8,
				y=v.y,
				dx=0,
				w=8,
				h=8,
				anim=0,
				active=true,
				positive=v.flipped
			})
		end 
	end
end

function draw_enemy_atk(enemy_attacks)
	for k,v in pairs(enemy_attacks) do
		if v.active then
			spr(v.sp,v.x,v.y,1,1)
		end
	end
end


