-- buffs

function draw_buff(level)
	for k,v in pairs(level.buffs) do
		if v.active then
			spr(9,v.x,v.y,1,1)
		end
	end
end

function update_buff(level)
	for k,v in pairs(level.buffs) do
		if v.active and buff_collision(v.x, v.y, player) then
			v.active=false
			player.lives+=1
		end
	end
end
