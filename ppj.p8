-- splash screen

function show_splash_screen()
	logo = {
		x=0,
		y=36,
	}
	text = {
		x=128,
		y=70,
	}
	max_x=48
	min_x=27
	game.upd = splash_screen_update
	game.drw = splash_screen_draw
end

function splash_screen_update()
	if time() > 4 then
		show_menu()
	end
end

function splash_screen_draw()
	cls()
	-- 2 sec for the pear
	if time() < 1 and logo.x<max_x then
		spr(76,logo.x,logo.y,4,4)
		logo.x+=1
	elseif time()>1.5 and time()<2 then
		spr(76,max_x,logo.y-8,4,4)
	elseif time()>2 then
		spr(76,max_x,logo.y,4,4)
	else
		spr(76,max_x,logo.y,4,4)
	end
	-- 1 sec for the text
	if time() < 1 and text.x>min_x then
		print("polar pear present",text.x,text.y,7)
		text.x-=2
	else
		print("polar pear present",min_x,text.y,7)
	end
end
